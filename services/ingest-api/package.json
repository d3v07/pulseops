{
    "name": "@pulseops/ingest-api",
    "version": "1.0.0",
    "private": true,
    "main": "dist/index.js",
    "scripts": {
        "dev": "tsx watch src/index.ts",
        "build": "tsc",
        "start": "node dist/index.js",
        "test": "jest --config jest.config.js",
        "test:unit": "jest --config jest.config.js --testPathPattern=unit",
        "test:integration": "jest --config jest.config.js --testPathPattern=integration",
        "test:coverage": "jest --config jest.config.js --coverage",
        "lint": "eslint src --ext .ts",
        "lint:security": "eslint src --ext .ts --rule 'no-eval: error'",
        "typecheck": "tsc --noEmit",
        "db:migrate": "node -r ts-node/register scripts/migrate.ts",
        "db:seed": "node -r ts-node/register scripts/seed.ts"
    },
    "dependencies": {
        "@fastify/cors": "^8.5.0",
        "@fastify/rate-limit": "^9.1.0",
        "bcrypt": "^5.1.1",
        "dotenv": "^16.4.5",
        "fastify": "^4.25.2",
        "ioredis": "^5.3.2",
        "jsonwebtoken": "^9.0.2",
        "kafkajs": "^2.2.4",
        "pg": "^8.11.3",
        "pino": "^8.17.2",
        "pino-pretty": "^10.3.1",
        "prom-client": "^15.1.3",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@jest/globals": "^29.7.0",
        "@types/bcrypt": "^5.0.2",
        "@types/jest": "^29.5.11",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/node": "^20.0.0",
        "@types/pg": "^8.10.9",
        "@types/supertest": "^6.0.3",
        "@vitest/coverage-v8": "^4.0.18",
        "jest": "^29.7.0",
        "supertest": "^6.3.4",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.2",
        "tsx": "^4.7.0",
        "vitest": "^1.1.1"
    }
}
